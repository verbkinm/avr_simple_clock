                                 ; managed by TM1637 microchip, available at Aliexpress a lot.
                                 ;
                                 ; How to code displaying information
                                 ;
                                 ; In order to display some information call TM1637_display and fill registers
                                 ; TM1637_d1 .. TM1637_d4 with letters, that are coded next way:
                                 ;         0
                                 ;       +---+
                                 ;     5 | 6 | 1
                                 ;       +---+
                                 ;     4 |   | 2
                                 ;       +---+
                                 ;         3
                                 ; 
                                 ; Here the ordering numers is the bits layout in letters that to be passed to the called function.
                                 ; Number one coded as 0b00000110
                                 ; Number two coded as 0b01011011
                                 ; So this way, use bit=1 for diods to be burnt and bit=0 for bits that to be off.
                                 ;
                                 
                                 ;========================================================
                                 ;				  
                                 ;========================================================	
                                 
                                 TM1637_display:
000176 930f                      	push	reg_1
000177 931f                      	push	reg_2
000178 932f                      	push	reg_3
                                 
                                 	;-------------------------     
                                 
000179 d043                      	rcall	TM1637_start
00017a e400                      	ldi		reg_1, 0x40			; Data command setting: Automatic address adding, Normal mode, Write data to display
00017b d016                      	rcall	TM1637_writeByte
00017c d044                      	rcall	TM1637_stop
                                 
                                 	;-------------------------   
                                 
00017d d03f                      	rcall	TM1637_start
00017e ec00                      	ldi		reg_1, 0xC0
00017f d012                      	rcall	TM1637_writeByte
                                 
                                 	;-------------------------      
                                 
000180 2f04                      	mov		reg_1, TM1637_d1
000181 d010                      	rcall	TM1637_writeByte
                                 
000182 2f05                      	mov		reg_1, TM1637_d2
000183 d00e                      	rcall	TM1637_writeByte
                                 
000184 2f06                      	mov		reg_1, TM1637_d3
000185 d00c                      	rcall	TM1637_writeByte
                                 
000186 2f07                      	mov		reg_1, TM1637_d4
000187 d00a                      	rcall	TM1637_writeByte
                                 
000188 d038                      	rcall	TM1637_stop
                                 
                                 	;-------------------------   
                                 
000189 d033                      	rcall	TM1637_start
00018a e80f                      	ldi		reg_1, 0x8f
00018b d006                      	rcall	TM1637_writeByte
00018c d034                      	rcall	TM1637_stop
00018d 0000                      	nop
                                 
00018e 912f                      	pop		reg_3
00018f 911f                      	pop		reg_2
000190 910f                      	pop		reg_1
                                 
000191 9508                      	ret
                                 
                                 ; Expected byte in reg_1
                                 ; reg_1 contains a char to be written is to be passed as an argument of the call
                                 ; used temp registers: reg_1, reg_2, reg_3
                                 ; params: reg_1 - incoming char (8-bit) that to be sent out
                                 
                                 ;========================================================
                                 ;					 
                                 ;========================================================
                                 
                                 TM1637_writeByte:
000192 e018                      	ldi		reg_2, 8
                                 
                                 	TM1637_writeByte_1:
000193 98c0                      		cbi		PORT_TM1367, TM1637_CLK
                                 
                                 	; starting if condition
000194 2f20                      		mov		reg_3, reg_1
000195 7021                      		cbr		reg_3, 0xfe
000196 3021                      		cpi		reg_3, 0x01
000197 f411                      		brne	TM1637_writeByte_send_low
                                 
                                 	TM1637_writeByte_send_high:
000198 9ac3                      		sbi		PORT_TM1367, TM1637_DATA
000199 c002                      		rjmp	TM1637_writeByte_sync
                                 
                                 	TM1637_writeByte_send_low:
00019a 98c3                      		cbi		PORT_TM1367, TM1637_DATA 
00019b c000                      		rjmp	TM1637_writeByte_sync
                                 
                                 	TM1637_writeByte_sync:
00019c 0000                      		nop
00019d 0000                      		nop
00019e 0000                      		nop
00019f 0000                      		nop
0001a0 0000                      		nop
0001a1 9506                      		lsr		reg_1 
0001a2 9ac0                      		sbi		PORT_TM1367, TM1637_CLK                    
0001a3 0000                      		nop
0001a4 0000                      		nop
0001a5 0000                      		nop
0001a6 0000                      		nop
0001a7 0000                      		nop
                                 
0001a8 951a                      		dec		reg_2
0001a9 3010                      		cpi		reg_2, 0                            ; end of 8-bit loop
0001aa f741                      		brne	TM1637_writeByte_1
                                 
0001ab 98c0                      		cbi		PORT_TM1367, TM1637_CLK
0001ac 0000                      		nop
0001ad 0000                      		nop
0001ae 0000                      		nop
0001af 0000                      		nop
0001b0 0000                      		nop
0001b1 98bb                      		cbi		DDR_TM1367, TM1637_DATA
                                 
                                 	TM1637_writeByte_wait_ACK:
0001b2 99b3                      		sbic	PIN_TM1367, TM1637_DATA
0001b3 cffe                      		rjmp	TM1637_writeByte_wait_ACK          ; wait for acknowledgment
                                     
0001b4 9abb                      		sbi		DDR_TM1367, TM1637_DATA
0001b5 9ac0                      		sbi		PORT_TM1367, TM1637_CLK
0001b6 0000                      		nop
0001b7 0000                      		nop
0001b8 0000                      		nop
0001b9 0000                      		nop
0001ba 0000                      		nop
0001bb 98c0                      		cbi		PORT_TM1367, TM1637_CLK
                                 
0001bc 9508                      	ret
                                 
